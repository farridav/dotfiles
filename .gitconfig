
[user]
	email = info@davidfarrington.co.uk
	name = David Farrington

[alias]
	dish = diff --staged
	hash = rev-parse --short HEAD
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	alias = config --get-regexp alias
	squash = rebase -i origin/develop
	udiff = diff HEAD...origin/develop
	
[push]
	default = current

[rebase]
	autosquash = true

[pull]
	default = current

[core]
	excludesfile = '~/.gitignore'